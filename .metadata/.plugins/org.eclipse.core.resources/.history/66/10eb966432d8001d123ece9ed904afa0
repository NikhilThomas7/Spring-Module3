package com.ust.iocautowiring;

public class B {
	A a;
	
	public B(A a) {
		this.a=a;
		System.out.println("from b() constructor");
		// TODO Auto-generated constructor stub
	}
	public void setA(A a) {
		System.out.println("setA() called");
		this.a = a;
	}
	public void setUp() {
		System.out.println("from d=setup");
	}
	
	public void windUp() {
		System.out.println("from d=setup");
	}
}
