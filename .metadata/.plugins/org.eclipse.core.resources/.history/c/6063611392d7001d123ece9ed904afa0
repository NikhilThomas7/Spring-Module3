package com.ust.ioc2;

public class Line {
	
	Point p1;
	Point P2;
	
	
	public Point getP1() {
		return p1;
	}


	public void setP1(Point p1) {
		this.p1 = p1;
	}


	public Point getP2() {
		return P2;
	}


	public void setP2(Point p2) {
		P2 = p2;
	}


	public void drawline() {
		System.out.println("from"+p1.getX());
	}
	

}
